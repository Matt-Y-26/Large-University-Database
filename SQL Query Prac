SELECT GENDER, AVG(AGE)
FROM STUDENT
GROUP BY GENDER;

SELECT LAST_NAME, AGE
FROM STUDENT
GROUP BY LAST_NAME, AGE
HAVING AGE > 20;

SELECT Prog_ID, Collge, Prog_Name, Student_ID
FROM PROGRAM
WHERE COLLEGE = ALL
  (SELECT College
  FROM ADMIN
  WHERE College = 'math_sch');
 
SELECT ADMIN_NAME
FROM ADMIN
WHERE COLLEGE IN (
  SELECT COLLEGE
  FROM PROGRAM
  WHERE COLLEGE='bus_sch');
  
CREATE VIEW "21 or Older Students" AS
SELECT FIRST_NAME, LAST_NAME, AGE
FROM STUDENT
WHERE Age > 20;
